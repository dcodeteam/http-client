{
  "private": false,
  "license": "MIT",
  "version": "0.0.9",
  "name": "@dc0de/http-client",
  "description": "Simple HttpClient built on top of Axios and RxJS",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "typings": "typings/index.d.ts",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "repository": "https://github.com/dcodeteam/http-client.git",
  "keywords": [
    "rxjs",
    "axios"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "all": "yarn lint --fix && yarn dts && yarn cover -u && yarn build",
    "ci": "yarn lint && yarn dts && yarn cover --ci && yarn build",
    "dts": "tsc --noEmit",
    "lint": "js-scripts lint",
    "test": "jest --watch",
    "cover": "jest --coverage",
    "build": "yarn build:es && yarn build:cjs",
    "build:es": "rollup -c ./config/rollup.es.js",
    "build:cjs": "rollup -c ./config/rollup.cjs.js",
    "codecov": "codecov -f ./coverage/lcov.info"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "path-to-regexp": "^2.4.0",
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "rxjs": "^6.3.0"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.4.4",
    "@dc0de/eslint-config-base": "^0.4.1",
    "@dc0de/eslint-config-node": "^0.4.4",
    "@dc0de/js-scripts": "^0.3.1",
    "@types/jest": "^23.3.5",
    "codecov": "^3.1.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rollup": "^0.66.6",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "rxjs": "^6.3.3",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.2",
    "typescript-eslint-parser": "^20.0.0"
  }
}
