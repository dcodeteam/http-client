{
  "private": false,
  "license": "MIT",
  "version": "0.0.1",
  "name": "@dc0de/http-client",
  "description": "Simple HttpClient built on top of Axios and RxJS",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "typings": "typings/index.d.ts",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "repository": "https://github.com/dcodeteam/http-client.git",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "all": "yarn lint --fix && yarn dts && yarn cover && yarn build",
    "ci": "yarn lint && yarn dts && yarn cover --ci && yarn build",
    "dts": "tsc --noEmit",
    "lint": "js-scripts lint",
    "test": "jest --watch",
    "cover": "yarn test --no-watch --coverage",
    "build": "yarn build:es && yarn build:cjs",
    "build:es": "rollup -c ./config/rollup.es.js",
    "build:cjs": "rollup -c ./config/rollup.cjs.js",
    "codecov": "codecov -f ./coverage/lcov.info"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "path-to-regexp": "^2.2.1",
    "qs": "^6.5.2"
  },
  "peerDependencies": {
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.3.0",
    "@dc0de/eslint-config-base": "^0.3.0",
    "@dc0de/eslint-config-node": "^0.3.0",
    "@dc0de/js-scripts": "^0.1.12",
    "@types/jest": "^23.3.1",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "rollup": "^0.64.1",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rxjs": "^6.2.2",
    "source-map-support": "^0.5.8",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
